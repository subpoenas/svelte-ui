{
  "name": "SvelteUI",
  "version": "1.0.0",
  "description": "FlatUI using Webpack 4",
  "main": "index.js",
  "scripts": {
    "local": "cross-env TARGET_ENV='local' webpack-dev-server --no-inline --config build/webpack.config.local.js",
    "dev1": "node build/build.js",
    "dev": "cross-env TARGET_ENV='development' node build/build.js",
    "stg": "cross-env TARGET_ENV='stage' node build/build.js",
    "prd": "cross-env TARGET_ENV='production' node build/build.js",
    "test:unit": "jest --config test/jest.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --config test/jest.config.js"
  },
  "keywords": [
    "Svelte",
    "Webpack 4"
  ],
  "author": "Alton.Jung",
  "license": "MIT",
  "dependencies": {
    "moment": "^2.24.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.10",
    "svelte": "3.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "chalk": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-notifier": "^5.4.0",
    "node-sass": "^7.0.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.1.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svelte-loader": "^2.13.4",
    "uglify-es": "^3.3.9",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 3.0.0"
  }
}
